{
  "name": "site",
  "version": "1.0.0",
  "description": "A design system built with Storybook, TailwindCSS and Drupal",
  "main": "",
  "private": true,
  "license": "GPL-2.0",
  "engines": {
    "node": ">=14.17.5",
    "yarn": ">=1.3.2"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "prestart": "[ -d 'node_modules' ] || npm i",
    "preinstall": "rm -rf node_modules/**/.git/",
    "start": "npm run dev:drupal-storybook",
    "dev": "npm run ws dev",
    "dev:storybook:tpx-sk-sitename-kebab": "npm run dev storybook-tpx-sk-sitename-kebab",
    "dev:storybook:tpx-sk-subsitename-kebab": "npm run dev storybook-tpx-sk-subsitename-kebab",
    "dev:drupal:tpx-sk-sitename-kebab": "npm run dev drupal-tpx-sk-sitename-kebab",
    "dev:drupal:tpx-sk-subsitename-kebab": "npm run dev drupal-tpx-sk-subsitename-kebab",
    "dev:docs": "npm run dev docs-tpx-sk-sitename-kebab",
    "build": "npm run ws build",
    "build:storybook:tpx-sk-sitename-kebab": "npm run build storybook-tpx-sk-sitename-kebab",
    "build:storybook:tpx-sk-subsitename-kebab": "npm run build storybook-tpx-sk-subsitename-kebab",
    "build:drupal:tpx-sk-sitename-kebab": "npm run build drupal-tpx-sk-sitename-kebab",
    "build:drupal:tpx-sk-subsitename-kebab": "npm run build drupal-tpx-sk-subsitename-kebab",
    "build:docs": "npm run build docs-tpx-sk-sitename-kebab",
    "publish:storybook": "npx chromatic -d docroot/storybook",
    "lint:js": "eslint --ext .js ./src",
    "lint:css": "stylelint '**/*.css'",
    "lint": "npm run lint:js; npm run lint:css;",
    "fmt:js": "eslint --fix --ext .js,.vue ./src/design-systems",
    "fmt:css": "npm run lint:css --fix && prettier --loglevel warn --write '**/*.{css}'",
    "fmt": "npm run fmt:js; npm run fmt:css",
    "ci": "npm run fmt && npm run build:drupal:tpx-sk-subsitename-kebab",
    "ws": "ws",
    "start-storybook": "start-storybook",
    "prettier": "prettier",
    "chromatic": "npx chromatic"
  },
