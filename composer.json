{
    "name": "drupal/recommended-project",
    "description": "tpx_sk_sitename site codebase.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "browserstate/history.js",
                "version": "1.8.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/browserstate/history.js/archive/refs/tags/1.8.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    ],
    "require": {
        "acquia/memcache-settings": "^1.0",
        "browserstate/history.js": "^1.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_connector": "^4.0",
        "drupal/acquia_purge": "^1.1",
        "drupal/acquia_search": "^3.0.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/allowed_formats": "^2.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/autocomplete_deluxe": "^2.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/captcha": "^1.2",
        "drupal/components": "^3.0@beta",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^1.7",
        "drupal/content_moderation_notifications": "^3.5",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/crop": "^2.3",
        "drupal/default_content": "^2.0@alpha",
        "drupal/double_field": "^4.1",
        "drupal/editor_advanced_link": "^2.1",
        "drupal/entity_browser": "^2.9",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/entityqueue": "^1.6",
        "drupal/extra_field": "^2.1",
        "drupal/extra_field_plus": "^3.0@beta",
        "drupal/facets": "^3.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.0",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_lb": "^1.0@RC",
        "drupal/gin_login": "^2.0",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/google_tag": "^2.0",
        "drupal/honeypot": "^2.1",
        "drupal/iframeremove": "^2.0",
        "drupal/imagemagick": "^3.2",
        "drupal/jquery_ui": "^1.6",
        "drupal/layout_builder_browser": "^1.5",
        "drupal/link_attributes": "^1.13",
        "drupal/linkit": "^6.0@RC",
        "drupal/mailsystem": "^4.3",
        "drupal/maxlength": "^2.1",
        "drupal/media_entity_download": "^2.2",
        "drupal/media_library_form_element": "^2.0",
        "drupal/metatag": "^1.23",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/override_node_options": "^2.7",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_ee": "^2.1@alpha",
        "drupal/paragraphs_features": "^2.0@beta",
        "drupal/pathauto": "^1.8",
        "drupal/purge": "^3.0",
        "drupal/qa_accounts": "^1.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.2",
        "drupal/redirect": "^1.6",
        "drupal/scheduler": "^2.0@RC",
        "drupal/scheduler_content_moderation_integration": "^2.0@beta",
        "drupal/search_api": "^1.29",
        "drupal/search_api_solr": "^4.2",
        "drupal/simple_sitemap": "^4.1",
        "drupal/site_settings": "^1.20",
        "drupal/smtp": "^1.2",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/structure_sync": "^2.0",
        "drupal/svg_image": "^3.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/ui_patterns": "^1.5",
        "drupal/ui_patterns_extends": "^1.0@beta",
        "drupal/ui_patterns_layout_builder": "^1.2",
        "drupal/ui_patterns_settings": "^2.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.0",
        "drupal/username_enumeration_prevention": "^1.3",
        "drupal/view_mode_switch": "^2.0",
        "drupal/views_ajax_history": "^1.7",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/webform": "^6.2@beta",
        "drupal/wingsuit_companion": "^2.1",
        "drush/drush": "^11",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.1",
        "drupal/twig_xdebug": "^1.3",
        "kint-php/kint": "^3.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "overwrite": true,
            "file-mapping": {
                "[web-root]/.csslintrc": false,
                "[web-root]/.editorconfig": false,
                "[web-root]/.eslintignore": false,
                "[web-root]/.eslintrc.json": false,
                "[web-root]/.gitattributes": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.txt": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/update.php": false,
                "[web-root]/web.config": false,
                "[project-root]/.gitignore": false,
                "[project-root]/.gitattributes": false,
                "[project-root]/.editorconfig": false,
                "[web-root]/sites/default/settings.php": "src/settings/default/settings.php",
                "[web-root]/sites/development.services.yml": "src/settings/development.services.yml",
                "[web-root]/sites/tpx_sk_subsitename_snake/settings.php": "src/settings/tpx_sk_subsitename_snake/settings.php"
            }
        },
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-install-cmd-message": [
                "<bg=yellow;fg=black>                                        </>",
                "<bg=yellow;fg=black> Welcome to tpx_sk_sitename codebase    </>",
                "<bg=yellow;fg=black>                                        </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Spin the site(s) up with <bg=yellow;fg=black>make up</>",
                "  * Seed the database with <bg=yellow;fg=black>make seed-db</>",
                "  * Build the Drupal frontend with <bg=yellow;fg=black>make front-end</>",
                "  * Login to the site with <bg=yellow;fg=black>make uli</>",
                "  * List all available Make utilities with <bg=yellow;fg=black>make help</>"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "Issue #2321071 - BaseFieldOverride error": "https://www.drupal.org/files/issues/2019-12-24/2321071-39.patch",
                "Issue #3114467 - Condition plugin issue": "https://www.drupal.org/files/issues/2021-11-18/3114467-25.patch"
            },
            "drupal/redirect": {
                "Issue #3057250 - Validation issue on adding url redirect": "https://www.drupal.org/files/issues/2022-09-01/3057250-53.patch"
            },
            "drupal/wingsuit_companion": {
                "Add default theme as a UI patterns provider": "./src/patches/wingsuit_companion/add-default-theme-as-pattern-provider.patch"
            }
        }
    },
    "scripts": {
        "link-src-files": [
            "mkdir -p docroot/profiles && mkdir -p docroot/modules && mkdir -p docroot/themes",
            "ln -sfn ../../src/settings/sites.php docroot/sites",
            "ln -sfn ../../src/modules docroot/modules/custom",
            "ln -sfn ../../src/themes docroot/themes/custom"
        ],
        "phpcs": [
            "vendor/bin/phpcs --standard=Drupal,DrupalPractice src/modules"
        ],
        "post-drupal-scaffold-cmd": [
            "@composer link-src-files",
            "[ -f 'src/patches/htaccess.patch' ] && patch -p2 docroot/.htaccess < src/patches/htaccess.patch || echo 'Skipping htaccess patch'"
        ],
        "post-install-cmd": [
            "@composer link-src-files"
        ],
        "post-update-cmd": [
            "@composer link-src-files"
        ]
    }
}
